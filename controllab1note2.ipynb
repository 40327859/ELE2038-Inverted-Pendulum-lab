    "M = 0.3\n",
    "m = 0.1\n",
    "ell = 35*10**-2\n",
    "g = 9.81\n",
    "\n",
    "w = ell*(4*M +m)\n",
    "c = 3/w\n",
    "d = c*(M+m) * g\n",
    "\n",
    "s = ctrl.TransferFunction.s\n",
    "G = (-1*c)/(s**2 - d)\n",
    "\n",
    "ts = np.linspace(0,0.1,200)\n",
    "u = 0.05 * ts**1.8 * np.cos(ts)\n",
    "\n",
    "\n",
    "rsp = ctrl.forced_response(G,ts,u)\n",
    "plt.plot(ts,u)\n",
    "plt.plot(resp.time, resp.outputs)\n",
    "plt.show()"
   ]
